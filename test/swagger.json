  "paths": {
    "/api/cloud/cloudFile/uploadFile": {
      "post": {
        "tags": [
          "cloudFile"
        ],
        "summary": "上传文件",
        "description": "## 经测试100M网络情况好的状况下上传20s左右\r\n## 可选参数 isCallBack：是否回调进度 默认：false",
        "operationId": "api-cloud-cloudFile-uploadFile-Post",
        "parameters": [
          {
            "name": "isCallBack",
            "in": "query",
            "schema": {
              "type": "boolean"
            }
          }
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "file"
                ],
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "file": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "text/plain": {
                "schema": {
                  "$ref": "#/components/schemas/AdminResult_FileOutput"
                }
              },
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/AdminResult_FileOutput"
                }
              },
              "text/json": {
                "schema": {
                  "$ref": "#/components/schemas/AdminResult_FileOutput"
                }
              }
            }
          }
        }
      }
    },
